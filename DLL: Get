public int size(){
            return size;
}
public boolean isEmpty(){
            if(size == 0){
                return true;
            }
            return false;
}
 public int getFirst(){
            if(head == null){
                return -1;
            }
            return head.val;
 }
 public int getLast(){
            if(head == null){
                return -1;
            }
            return tail.val;
 }
